        <script id="cd-script" type="text/javascript">

            function truncWithEllipsis(input) {
                if (input && input.length > 150) return input.substring(0, 149) + '…';
                return input;
            }

            function setCdValues(name) {
                var listOfCustomDimantions = [{"name":"topic","value":"אכיפה,מידע לצרכנים,מידע לעוסקים"},{"name":"office","value":"הרשות להגנת הצרכן ולסחר הוגן"}];
                listOfCustomDimantions.forEach(function (element)
                { return setCdByType(element, name) });
            }

            function setCdByType(item,setName) {
               switch (item.name)
                {
                    case "office":

                    gtag((setName ? setName + '.' : '') + 'set', { 'dimension1': truncWithEllipsis(item.value) });
                        break;
                    case "topic":

                    gtag((setName ? setName + '.' : '') + 'set', { 'dimension2': truncWithEllipsis(item.value) });
                        break;
                    case "units":

                    gtag((setName ? setName + '.' : '') + 'set', { 'dimension3': truncWithEllipsis(item.value) });
                        break;
                    default:
                        break;
                }
            }

        </script>


    


